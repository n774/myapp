<h1>Listing rates</h1>

<table>
  <tr>
    <th>Groupid</th>
    <th>Rateid</th>
    <th>Brateid</th>
    <th>Point</th>
    <th>Comment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rates.each do |rate| %>
  <tr>
    <td><%= rate.g_id %></td>
    <td><%= rate.rate_id %></td>
    <td><%= rate.brate_id %></td>
    <td><%= rate.point %></td>
    <td><%= rate.comment %></td>
    <td><%= link_to 'Edit', edit_rate_path(rate) %></td>

  </tr>
<% end %>
</table>

